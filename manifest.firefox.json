{
  "manifest_version": 3,
  "name": "Joblife",
  "version": "2.9.0.1",
  "description": "Suivez l'actualité des matchs, des nouvelles vidéos, des streams des joueurs et des annonces de l'équipe Joblife.",
  "action": {
    "default_popup": "src/popup/index.html",
    "default_title": "Joblife"
  },
  "background": {
    "scripts": [
      "src/background/background.ts"
    ]
  },
  "permissions": [
    "storage",
    "notifications",
    "alarms"
  ],
  "host_permissions": [
    "*://*.asakicorp.com/*",
    "*://*.youtube.com/*"
  ],
  "icons": {
    "16": "assets/logo/icon-16.png",
    "32": "assets/logo/icon-32.png",
    "48": "assets/logo/icon-48.png",
    "128": "assets/logo/icon-128.png",
    "256": "assets/logo/icon-256.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "tsuyodevcontact@gmail.com"
    }
  }
}